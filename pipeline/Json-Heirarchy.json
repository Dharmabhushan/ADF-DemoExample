{
	"name": "Json-Heirarchy",
	"properties": {
		"activities": [
			{
				"name": "Cpy_Herarchy_Json",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['sensor_id']"
								},
								"sink": {
									"name": "sensor_id",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "$['location']['building']"
								},
								"sink": {
									"name": "building"
								}
							},
							{
								"source": {
									"path": "$['location']['floor']"
								},
								"sink": {
									"name": "floor"
								}
							},
							{
								"source": {
									"path": "$['location']['room']"
								},
								"sink": {
									"name": "room"
								}
							},
							{
								"source": {
									"path": "$['readings']['temperature']"
								},
								"sink": {
									"name": "temperature"
								}
							},
							{
								"source": {
									"path": "$['readings']['humidity']"
								},
								"sink": {
									"name": "humidity"
								}
							},
							{
								"source": {
									"path": "$['timestamp']"
								},
								"sink": {
									"name": "timestamp",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DS_JsonSimple",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_AzureSQLJSON",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2025-10-03T10:43:05Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}